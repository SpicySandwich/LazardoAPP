package com.client.LazardoClient.DAO;

import org.apache.ibatis.annotations.Mapper;
import org.apache.ibatis.annotations.Param;
import org.apache.ibatis.annotations.Update;

@Mapper
public interface ChangeRole {
	
	final String CHANGE_ROLE = "UPDATE tbcliendetails  cd "
													+ "SET cd.role = #{roleChange} "
													+ "WHERE cd.client_email = #{} ";
	
	@Update( CHANGE_ROLE)
	Integer changeClientRole(@Param("role") ChangeRole role);

}
