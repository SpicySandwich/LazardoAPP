package com.client.LazardoClient.Model;

import com.fasterxml.jackson.annotation.JsonProperty;
import com.fasterxml.jackson.annotation.JsonProperty.Access;

import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Data;
import lombok.NoArgsConstructor;


@NoArgsConstructor
@Data
@Builder
public class ClientLogin {
	
	@JsonProperty(access = Access.WRITE_ONLY)
	private Integer loginId;
	
	private String username;
	
	@JsonProperty(access = Access.WRITE_ONLY)
	private String reTypeUsername;
	
	private String password;
	
	@JsonProperty(access = Access.WRITE_ONLY)
	private String reTypePassword;
	
	private ClientDetails clientDetails;

	public static ClientLogin(Integer loginId, String username, String reTypeUsername, String password, String reTypePassword,
			ClientDetails clientDetails) {
		super();
		this.loginId = loginId;
		this.username = username;
		this.reTypeUsername = reTypeUsername;
		this.password = password;
		this.reTypePassword = reTypePassword;
		this.clientDetails = clientDetails;
	}
	
	

}


