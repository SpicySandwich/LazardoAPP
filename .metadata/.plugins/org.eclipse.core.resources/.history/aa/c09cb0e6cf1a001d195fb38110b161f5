package com.client.LazardoClient.DAO;

import org.apache.ibatis.annotations.Mapper;
import org.apache.ibatis.annotations.Update;

@Mapper
public interface ChangeRole {
	
	final String CHANGE_ROLE = "UPDATE tbcliendetails  cd\r\n"
													+ "SET cd.role = 2\r\n"
													+ "WHERE cd.client_email = 'conred@gmail.com';";
	
	@Update( CHANGE_ROLE)
	Integer changeClientRole(Integer role);

}
